{
  "name": "transbank/transbank-sdk",
  "description": "Transbank SDK",
  "version": "1.5.6",
  "keywords": ["payments", "transbank", "api", "sdk"],
  "require": {
      "php": ">=5.5.0",
      "ext-curl": "*",
      "ext-json": "*",
      "ext-mbstring": "*"
    },
    "require-dev": {
      "phpunit/phpunit": "~4.0",
      "satooshi/php-coveralls": "~0.6.1",
      "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
      "psr-4": {
        "Transbank\\Onepay\\" : ["lib/onepay", "lib/onepay/utils"],
        "Transbank\\Onepay\\Exceptions\\" : ["lib/onepay/exceptions"],
        "Transbank\\Webpay\\" : ["lib/webpay", "lib/webpay/soap"],
        "Transbank\\PatPass\\" :  ["lib/patpass"],
        "Transbank\\PatPass\\PatpassComercio\\" :  ["lib/patpass/patpass_comercio"],
        "Transbank\\PatPass\\Exceptions\\" : ["lib/patpass/exceptions"],
      }
    },
    "license": "BSD-3-Clause"
}
