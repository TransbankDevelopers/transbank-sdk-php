{
  "name": "transbank/transbank-sdk",
  "description": "Transbank SDK",
  "keywords": [
    "payments",
    "transbank",
    "api",
    "sdk"
  ],
  "require": {
    "php": ">=5.5.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "satooshi/php-coveralls": "~0.6.1",
    "squizlabs/php_codesniffer": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "Transbank\\Onepay\\": [
        "src/Onepay",
        "src/Onepay/utils"
      ],
      "Transbank\\Onepay\\Exceptions\\": [
        "src/Onepay/exceptions"
      ],
      "Transbank\\Webpay\\": [
        "src/Webpay",
        "src/Webpay/soap"
      ]
    }
  },
  "license": "BSD-3-Clause"
}
